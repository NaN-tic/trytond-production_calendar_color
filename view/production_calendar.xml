<data>
    <xpath expr="/calendar" position="replace_attributes">
      <calendar dtstart="planned_start_date" dtend="planned_date" color="calendar_color" background_color="calendar_background_color"/>
    </xpath>
    <xpath expr="/calendar/field[@name='reference']" position="after">
        <field name="quantity"/>
    </xpath>
</data>